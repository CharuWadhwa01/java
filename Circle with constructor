class Circle{
    
    double r;

double circum(){
    return  2*3.14*r;
}
double area(){
    return (3.14)*r*r;
}
Circle(double a,double b, double c){
    r=a;
    r=b;
    r=c;
}
}

class Demo{
    public static void main(String args[]){
        Circle c=new Circle (3.0,4.0,6.0);
        System.out.println("Circum" + c.circum());
        System.out.println("Area" + c.area());
    }
}
